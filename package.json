{
    "name": "reseply",
    "version": "1.0.0",
    "private": true,
    "description": "Jelajahi cita rasa kuliner indonesia,  Temukan ribuan resep masakan nusantara yang autentik. Dari hidangan tradisional hingga kreasi modern, setiap resep dirancang untuk memanjakan lidah Anda.",
    "author": "Kidzeroll",
    "license": "MIT",
    "homepage": "reseply.kidzeroll.space",
    "repository": {
        "type": "git",
        "url": "https://github.com/kidzeroll/reseply.git"
    },
    "keywords": [
        "nextjs",
        "tailwindcss",
        "shadcn",
        "typescript",
        "react",
        "better-auth",
        "prisma",
        "postgresql"
    ],
    "type": "module",
    "scripts": {
        "dev": "next dev --webpack",
        "build": "next build --webpack",
        "start": "next start",
        "lint": "biome check",
        "format": "biome format --write",
        "db:generate": "bunx --bun prisma generate",
        "db:push": "bunx --bun prisma db push",
        "db:pull": "bunx --bun prisma db pull",
        "db:seed": "bunx --bun prisma db seed",
        "db:studio": "bunx --bun prisma studio",
        "db:migrate": "bunx --bun prisma migrate dev",
        "db:migrate:deploy": "bunx --bun prisma migrate deploy",
        "db:migrate:reset": "bunx --bun prisma migrate reset",
        "db:format": "bunx --bun prisma format",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@ai-sdk/openai": "^3.0.29",
        "@ai-sdk/react": "^3.0.88",
        "@base-ui/react": "^1.1.0",
        "@better-auth/passkey": "^1.4.18",
        "@openrouter/ai-sdk-provider": "^2.2.3",
        "@prisma/adapter-pg": "^7.4.0",
        "@prisma/client": "^7.4.0",
        "@tanstack/react-form": "^1.28.0",
        "@tanstack/react-query": "^5.90.21",
        "@tanstack/react-query-devtools": "^5.91.3",
        "@tanstack/react-table": "^8.21.3",
        "@vercel/blob": "^2.2.0",
        "ai": "^6.0.86",
        "better-auth": "^1.4.18",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.4",
        "embla-carousel-autoplay": "^8.6.0",
        "embla-carousel-react": "^8.6.0",
        "framer-motion": "^12.34.0",
        "input-otp": "^1.4.2",
        "lucide-react": "^0.563.0",
        "next": "16.1.6",
        "next-themes": "^0.4.6",
        "pg": "^8.18.0",
        "qrcode": "^1.5.4",
        "radix-ui": "^1.4.3",
        "react": "19.2.3",
        "react-day-picker": "^9.13.2",
        "react-dom": "19.2.3",
        "react-resizable-panels": "^4",
        "recharts": "2.15.4",
        "resend": "^6.9.2",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.4.0",
        "vaul": "^1.1.2",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.0",
        "@faker-js/faker": "^10.3.0",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/pg": "^8.16.0",
        "@types/qrcode": "^1.5.6",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "prisma": "^7.4.0",
        "shadcn": "^3.8.4",
        "tailwindcss": "^4",
        "tsx": "^4.21.0",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5"
    },
    "ignoreScripts": [
        "sharp",
        "unrs-resolver"
    ],
    "trustedDependencies": [
        "sharp",
        "unrs-resolver"
    ]
}
